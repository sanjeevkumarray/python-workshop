//question5:-------

def compare(a):
    ctr = 0
    for i in a:
        if len(i) > 2 and i[0] == i[-1]:
            ctr+= 1
    return ctr

a = ['abc', 'xyz', 'aba', '1221']
for i in a:
    z = compare(a)

print(z)

//question6:-----

a=[(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]
for j in range (1,len(a)) :
for i in range(len(a)-j) :
if a[i][1]>a[i+1][1] :
temp=a[i]
a[i]=a[i+1]
a[i+1]=temp
for i in range(len(a)) :
print(a[i])

