    //Question  1:
    def mul_Num(a,b):
    multiply=a*b;
    return multiply;
num1=25
num2=55
print("The product is",mul_Num(num1,num2))


//Question 2:
def add_num(a,b):
    sum=a+b;
    return sum;
num1=57  
num2=59
print("The sum is",add_num(num1,num2))#call the function
